<section class="section">
  <div class="container">
    <div class="has-text-centered">

      <h1 class="title"> Result.<%= @result_id %></h1>

      <table class="table is-striped" style="margin: 0 auto;" >
        <thead>
          <tr>
            <th>Model</th>
            <th>Score</th>
            <th>Time</th>
          </tr>
        </thead>

        <tbody>
          <% @results.to_a.each do |res| %>
            <tr>
              <td><%= res[:id] %></td>
              <td><%= res[:score].to_f %></td>
              <td><%= res[:time].to_f %></td>
            </tr>
          <% end %>

        </tbody>
      </table>

    </div>
  </div>
</section>
